{
  "env": {
    "development": {
      "presets": [
        "@babel/preset-es2015",
        "@babel/preset-react",
        "@babel/preset-stage-1",
        ["@babel/preset-env",{
          "useBuiltIns": "entry"
        }]
      ],
      "plugins": [["react-hot-loader/babel"],["@babel/plugin-transform-classes"]]
    },
    "test": {
      "presets": [["@babel/preset-env"], "@babel/preset-react"]
    },
    "production": {
      "presets": [
        "@babel/preset-es2015",
        "@babel/preset-react",
        "@babel/preset-stage-1",
        ["@babel/preset-env",{
          "useBuiltIns": "entry"
        }]
      ],
      "plugins": [
        "@babel/plugin-transform-classes",
        "@babel/plugin-transform-react-constant-elements",
        "babel-plugin-transform-react-remove-prop-types"
      ]
    }
  }
}
