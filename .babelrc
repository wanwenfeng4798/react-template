{
  "presets": [
    "react",
    "stage-1",
    "es2015"
  ],
  "env": {
    "development": {
      "plugins": [["react-hot-loader/babel"],["transform-class-properties"]]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-remove-prop-types",
        "transform-class-properties"
      ]
    }
  }
}
