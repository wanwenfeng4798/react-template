{
  "name": "react-template",
  "version": "1.0.9",
  "description": "这是一个最新版的react模板,会一直维护更新,因为没有类似vue-cli那样的工具所以自己建了个",
  "engines": {
    "npm": ">=8"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "dev": "vite --open --force",
    "build:vite": "npm run remove-dist && vite build --mode production",
    "start-message": "node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel  open:src lint:watch test:watch",
    "open:src": " webpack serve --mode development",
    "open:dist": "node tools/distServer.js",
    "lint": "esw --ext .tsx,.jsx,.js,.ts src/ tools/ --fix --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "node tools/build.js  && npm run open:dist",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "keep_wan",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "echarts": "^5.4.3",
    "history": "^5.3.0",
    "js-cookie": "^3.0.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "redux": "^5.0.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/plugin-transform-async-generator-functions": "^7.23.4",
    "@babel/plugin-transform-async-to-generator": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-json-strings": "^7.23.4",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/plugin-transform-react-constant-elements": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.5",
    "@loadable/component": "^5.15.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browser-sync": "^2.29.3",
    "chalk": "^4.1.2",
    "connect-history-api-fallback": "^2.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.33.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-watch": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-pxtorem": "^6.0.0",
    "react-refresh": "^0.14.0",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "unplugin-auto-import": "^0.17.2",
    "vite": "^5.0.5",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-cdn2": "^0.15.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "last 15 versions",
    "> 0.00001%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "react-template",
    "react-version18"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whevether/react-template.git"
  }
}
