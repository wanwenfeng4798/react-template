{
  "name": "react-template",
  "version": "0.2.9",
  "description": "这是一个最新版的react模板,会一直维护更新,因为没有类似vue-cli那样的工具所以自己建了个",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel  open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "open:serverDist": "babel-node tools/server/distServer.js",
    "open:srcServer": "babel-node tools/server/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "babel-node tools/build.js  && npm run open:dist",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "keep_wan",
  "license": "MIT",
  "dependencies": {
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "create-react-class": "15.6.3",
    "history": "4.7.2",
    "object-assign": "4.1.1",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-helmet": "5.2.0",
    "react-loadable": "5.5.0",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-async-generators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.3.4",
    "@babel/plugin-transform-classes": "7.3.4",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "^7.3.4",
    "autoprefixer": "9.4.9",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.3",
    "chalk": "2.4.2",
    "compression": "1.7.3",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "enzyme": "3.9.0",
    "eslint": "5.14.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-watch": "4.0.2",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.1.0",
    "jest-cli": "24.1.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "npm-run-all": "4.1.5",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "4.7.1",
    "regenerator-runtime": "0.13.1",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.2",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-middleware": "3.6.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-md5-hash": "0.0.6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "react-template",
    "react-version16"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whevether/react-template.git"
  }
}
